<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="main.java.utils.SuitListener"></listener>
    </listeners>
    <test name="Test Automation for Kite">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="projectName" value="kite"></parameter>
        <classes>
            <class name="test.java.kite.LoginTest"/>
        </classes>
    </test> <!-- Test -->

    <test name="Test Automation for Dart">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="projectName" value="dart"></parameter>
        <classes>
            <class name="test.java.dart.LoginTest"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->